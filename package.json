{
    "name": "simple-publish",
    "version": "0.1.0",
    "description": "* A simple template project for org-mode websites git clone this project to a new web site name and then add or edit org files as needed.",
    "main": "js/index.js",
    "scripts": {
        "clean-published": "rm -rf ./published",
        "clean-el": "rm ./publish-project.el",
        "clean-all": "npm run clean-el && npm run clean-published",
        "html": "emacs --batch --load publish-project.el --eval '(org-publish \"simple-publish\" nil)'",
        "html-all": "emacs --batch --load publish-project.el --eval '(org-publish \"simple-publish\" t)'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jskksj/simple-publish.git"
    },
    "author": "John Krumpotick",
